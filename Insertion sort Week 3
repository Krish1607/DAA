#include<stdio.h>
void insertion(int array[],int n, int *comp,int *shift)
{
int i,j,key=0;
j=i-1;
while(j>=0 && array[j]>key)
{
array[j+1]=array[j]>key)
*shift++;
*comp++;
}
}
int main()
{
int arr[]={3,8,-23,65,81,70,21};
int n=sizeof(arr)/sizeof(arr[0]);
int shift=0,comp=0;
insertion(arr,n,&comp,&shift);
printf("\n Sorted array: ");
for(int i = 0;i<n;i++)
printf("%d ",arr[i]);
printf("Number of comp:%d & Number of shifts:%d",comp,shift);
return 0;
}
